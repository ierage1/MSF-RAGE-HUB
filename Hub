local MainTab = Window:CreateTab("Basics", nil)
local MainSection = MainTab:CreateSection("Main")

Rayfield:Notify({
   Title = "Executed",
   Content = "MSF.Rage Gui",
   Duration = 6.5,
   Image = nil,
})

local Button = MainTab:CreateButton({
   Name = "InfiniteYield",
   Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/edgeiy/infiniteyield/master/source'),true))()
   end,
})

local Slider = MainTab:CreateSlider({
   Name = "Walkspeed Slider",
   Range = {16, 200},
   Increment = 1,
   Suffix = "Speed",
   CurrentValue = 16,
   Flag = "Slider1", 
   Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
   end,
})

local Button = MainTab:CreateButton({
   Name = "ESP non-gui",
   Callback = function()
		local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer


local function createESP(player)
    if player ~= LocalPlayer then
        player.CharacterAdded:Connect(function(character)
            -- Wait for the character's HumanoidRootPart
            local rootPart = character:WaitForChild("HumanoidRootPart")
            
            -- Create Highlight
            local highlight = Instance.new("Highlight")
            highlight.Adornee = character
            highlight.FillColor = Color3.fromRGB(255, 0, 0) -- Red
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- White
            highlight.FillTransparency = 0.5 -- 50% transparent
            highlight.OutlineTransparency = 0.2 -- 20% transparent
            highlight.Parent = character
            
            -- Create BillboardGui for Name
            local billboard = Instance.new("BillboardGui")
            billboard.Name = "NameTag"
            billboard.Adornee = rootPart
            billboard.Size = UDim2.new(0, 200, 0, 50)
            billboard.StudsOffset = Vector3.new(0, 3, 0)
            billboard.AlwaysOnTop = true
            
            local nameLabel = Instance.new("TextLabel")
            nameLabel.Parent = billboard
            nameLabel.Size = UDim2.new(1, 0, 1, 0)
            nameLabel.BackgroundTransparency = 1
            nameLabel.Text = player.Name
            nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            nameLabel.TextStrokeTransparency = 0
            nameLabel.Font = Enum.Font.SourceSansBold
            nameLabel.TextScaled = true
            
            billboard.Parent = character
            
            -- Create BoxHandleAdornment for ESP Box
            local box = Instance.new("BoxHandleAdornment")
            box.Name = "ESPBox"
            box.Adornee = rootPart
            box.Size = rootPart.Size + Vector3.new(0.5, 2, 0.5)
            box.Color3 = Color3.fromRGB(0, 255, 0) -- Green
            box.AlwaysOnTop = true
            box.ZIndex = 10
            box.Transparency = 0.5 -- 50% transparent
            box.Parent = character
        end)
    end
end

for _, player in pairs(Players:GetPlayers()) do
    createESP(player)
end

Players.PlayerAdded:Connect(createESP)

   end,
})



local UniversalTab = Window:CreateTab("Universal", nil)
local Section = UniversalTab:CreateSection("Universal")

local Button = UniversalTab:CreateButton({
   Name = "Aimbot",
   Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/discopro/open-source/main/dreamybull.lua'),true))()
   end,
})

local Button = UniversalTab:CreateButton({
   Name = "Esp",
   Callback = function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/cool83birdcarfly02six/UNIVERSALESPLTX/main/README.md'),true))()
   end,
})

